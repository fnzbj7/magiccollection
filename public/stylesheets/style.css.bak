.magiccard{
	width:100%;
	position: relative;
}

.nothave{
	-webkit-transition: opacity 0.2s ease-in-out;
	-moz-transition: opacity 0.2s ease-in-out;
	-ms-transition: opacity 0.2s ease-in-out;
	-o-transition: opacity 0.2s ease-in-out;
	opacity: 0.4;
}

.nowrap:hover > .nothave{
	-webkit-transition: opacity 0.07s ease-in-out;
	-moz-transition: opacity 0.07s ease-in-out;
	-ms-transition: opacity 0.07s ease-in-out;
	-o-transition: opacity 0.07s ease-in-out;
	 opacity: 1;
}

.nowrap{
	position: relative;
	z-index: 20;
	float: left;
	margin: 5px 3px;
}

.zoomIcon{
	position: absolute;
	top: 30px;
	margin-left: 10px;
	z-index: 21;
	opacity: 0;
	width: 30px;
}

.zoomIcon:hover{
	top: 31px;
	margin-left: 11px;
}

.zoomIcon:active{
	top: 32px;
	margin-left: 12px;
}

.amountIcon{
	position: absolute;
	z-index: 21;
	top: 97.4%;
	width: 15%;
	margin-left: 42.5%;
}

.nowrap:hover > .zoomIcon{
	-webkit-transition: opacity 0.65s ease-in-out;
	-moz-transition: opacity 0.65s ease-in-out;
	-ms-transition: opacity 0.65s ease-in-out;
	-o-transition: opacity 0.65s ease-in-out;
	 opacity: 1;
	 transition-delay:1s;
}

.pagination {
    padding: 20px;
	font: 14px/24px sans-serif;
	width: 550px;
	margin: auto;
	margin-bottom: 20px;
	margin-bottom: 30px;
}

.page {
    display: inline-block;
    padding: 0px 9px;
    margin-right: 4px;
    border-radius: 3px;
    border: solid 1px #c0c0c0;
    background: #e9e9e9;
    box-shadow: inset 0px 1px 0px rgba(255,255,255, .8), 0px 1px 3px rgba(0,0,0, .1);
    font-size: .875em;
    font-weight: bold;
    text-decoration: none;
    color: #717171;
    text-shadow: 0px 1px 0px rgba(255,255,255, 1);
	line-height: 24px;
}

.page:hover, .page.gradient:hover {
    background: #fefefe;
    background: -webkit-gradient(linear, 0% 0%, 0% 100%, from(#FEFEFE), to(#f0f0f0));
    background: -moz-linear-gradient(0% 0% 270deg,#FEFEFE, #f0f0f0);
	cursor:pointer;
}

.page.active {
    border: none;
    background: #616161;
    box-shadow: inset 0px 0px 8px rgba(0,0,0, .5), 0px 1px 0px rgba(255,255,255, .8);
    color: #f0f0f0;
    text-shadow: 0px 0px 3px rgba(0,0,0, .5);
}

.page.gradient {
    background: -webkit-gradient(linear, 0% 0%, 0% 100%, from(#f8f8f8), to(#e9e9e9));
    background: -moz-linear-gradient(0% 0% 270deg,#f8f8f8, #e9e9e9);
}



/* entire container, keeps perspective */
.flip-container {
	perspective: 1000px;
}
	/* flip the pane when hovered */
 .flip-container.hover .flipper {
		transform: rotateY(180deg);
	}

/* flip speed goes here */
.flipper {
	transition: 0.6s;
	transform-style: preserve-3d;
	position: relative;
}

/* hide back of pane during swap */
.front, .back {
	backface-visibility: hidden;

	position: absolute;
	top: 0;
	left: 0;
}

/* front pane, placed above back */
.front {
	z-index: 2;
	/* for firefox 31 */
	transform: rotateY(0deg);
}

/* back, initially hidden pane */
.back {
	transform: rotateY(180deg);
}
