        "@fortawesome/free-brands-svg-icons": "^5.14.0",
        "@fortawesome/free-solid-svg-icons": "^5.14.0",
        "@nestjs/common": "^7.4.4",
        "@nestjs/core": "^7.4.4",
        "@nestjs/jwt": "^7.1.0",
        "@nestjs/passport": "^7.1.0",
        "@nestjs/platform-express": "^7.4.4",
        "@nestjs/typeorm": "^7.1.3",
        "@ng-bootstrap/ng-bootstrap": "^7.0.0",
        "@popperjs/core": "^2.4.4",
        "@types/jest": "^26.0.13",
        "@types/moment-timezone": "^0.5.13",
        "@types/passport-jwt": "^3.0.3",
        "angularx-social-login": "^3.2.1",
        "bcrypt": "^5.0.0",
        "bootstrap": "^4.5.2",
        "class-transformer": "^0.3.1",
        "class-validator": "^0.12.2",
        "compression": "^1.7.4",
        "config": "^3.3.1",
        "core-js": "^3.6.5",
        "fs": "0.0.1-security",
        "husky": "^4.3.0",
        "jquery": "^3.5.1",
        "jwt-decode": "^2.2.0",
        "mailgun-js": "^0.22.0",
        "mysql": "^2.18.1",
        "ng-lazyload-image": "^9.0.1",
        "passport": "^0.4.1",
        "passport-facebook-token": "^4.0.0",
        "passport-jwt": "^4.0.0",
        "path": "^0.12.7",
        "pixi.js": "^5.3.3",
        "popper.js": "^1.16.1",
        "pretty-quick": "^3.0.1",
        "reflect-metadata": "^0.1.13",
        "rimraf": "^3.0.2",
        "rxjs": "^6.6.3",
        "tslib": "^2.0.1",
        "typeorm": "^0.2.26",
