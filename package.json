    "husky": {
        "hooks": {
            "pre-commit": "pretty-quick --staged"
        }
    },
    "lint-staged": {
        "*.ts": [
            "./node_modules/.bin/tslint"
        ]
    },
    "jest": {
        "moduleFileExtensions": [
            "js",
            "json",
            "ts"
        ],
        "rootDir": "src",
        "testRegex": ".spec.ts$",
        "transform": {
            "^.+\\.(t|j)s$": "ts-jest"
        },
        "coverageDirectory": "../coverage",
        "testEnvironment": "node"
    }
}
