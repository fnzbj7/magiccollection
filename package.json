{
    "name": "magiccollection",
    "version": "1.0.0",
    "description": "One day, i decided that, i want to train my webpage developing skills, and i need a system to organize my MagicÂ® The Gathering collections. <br/>So, that's when this git repo was made.",
    "main": "version.js",
    "scripts": {
        "build": "npm run back:build && npm run front:build",
        "start": "node dist/main.js",
        "postinstall": "node version.js && ngcc",
        "front:ng": "ng",
        "front:start": "ng serve --ssl --ssl-key .\\certificates\\localhost.key  --ssl-cert .\\certificates\\localhost.crt",
        "front:build": "ng build --prod",
        "front:test": "ng test",
        "front:posttest": "npm run format",
        "front:lint": "ng lint",
        "front:e2e": "ng e2e",
        "front:prettier": "prettier --write \"src/frontend/app/**/*.{ts,scss}\"",
        "front:format": "prettier --loglevel warn --write \"**/*.{js,css,md}\"",
        "back:build": "rimraf dist && tsc -p tsconfig.build.json",
        "back:format": "prettier --write \"src/**/*.ts\"",
        "back:start": "ts-node -r tsconfig-paths/register src/main.ts",
        "back:start:dev": "tsc-watch -p tsconfig.build.json --onSuccess \"node dist/main.js\"",
        "back:start:debug": "tsc-watch -p tsconfig.build.json --onSuccess \"node --inspect-brk dist/main.js\"",
        "back:start:prod": "node dist/main.js",
        "back:lint": "tslint -p tsconfig.backend.json -c tslint.json",
        "back:test": "jest",
        "back:test:watch": "jest --watch",
        "back:test:cov": "jest --coverage",
        "back:test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
        "back:test:e2e": "jest --config ./test/jest-e2e.json",
        "typeorm": "cd src/backend && node --require ts-node/register ../../node_modules/typeorm/cli.js",
        "typeorm:create": "cd src/backend && node --require ts-node/register ../../node_modules/typeorm/cli.js migration:generate -n migrationNameSe"
    },
    "dependencies": {
        "@angular/animations": "^10.1.1",
        "@angular/cdk": "^10.2.0",
        "@angular/common": "^10.1.1",
        "@angular/compiler": "^10.1.1",
        "@angular/core": "^10.1.1",
        "@angular/forms": "^10.1.1",
        "@angular/localize": "^10.1.1",
        "@angular/material": "^10.2.0",
